<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context" xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd"> <!-- bean definitions here -->

	<context:annotation-config/>
	
	
	
	<bean name="human" class="com.app.DIDemo6.Human"></bean>
	
	<bean name="humanHeart" class="com.app.DIDemo6.Heart">
	<property name="nameOfAnimal" value="human"></property>
	<property name="noOfHearts" value="1"></property>
	</bean>
	
	<bean name="octupusHeart" class="com.app.DIDemo6.Heart">
	<property name="nameOfAnimal" value="octupus"></property>
	<property name="noOfHearts" value="3"></property>
	</bean>
	
</beans>